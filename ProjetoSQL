--------------------------------------------------------
--  DDL for View VW_AUTORIA_OBRAS
--------------------------------------------------------

  CREATE OR REPLACE FORCE VIEW "A72201223"."VW_AUTORIA_OBRAS" ("ID_OBRA", "TITULO", "AUTOR_PRINCIPAL", "ORGANIZADOR") AS 
  SELECT EV_OBRA.ID_OBRA, EV_OBRA.TITULO, EV_AUTORIA.AUTOR_PRINCIPAL, EV_AUTORIA.ORGANIZADOR
FROM EV_AUTOR
INNER JOIN EV_AUTORIA ON EV_AUTOR.ID_CPF = EV_AUTORIA.ID_CPF
INNER JOIN EV_OBRA ON EV_AUTORIA.ID_OBRA = EV_OBRA.ID_OBRA
;
--------------------------------------------------------
--  DDL for View VW_INTERSECAO_AREA
--------------------------------------------------------

  CREATE OR REPLACE FORCE VIEW "A72201223"."VW_INTERSECAO_AREA" ("ID_OBRA", "ID_PARECERISTA", "ID_AREA") AS 
  SELECT AO.ID_OBRA, AP.ID_PARECERISTA, AP.ID_AREA
FROM EV_AREA_OBRA AO
INNER JOIN EV_AREA_PARECERISTA AP ON AO.ID_AREA = AP.ID_AREA
ORDER BY AP.ID_PARECERISTA
;
--------------------------------------------------------
--  DDL for View VW_MEDIA_NOTAS_OBRA
--------------------------------------------------------

  CREATE OR REPLACE FORCE VIEW "A72201223"."VW_MEDIA_NOTAS_OBRA" ("ID_OBRA", "MEDIA_NOTAS") AS 
  SELECT ID_OBRA, AVG(NOTA) AS MEDIA_NOTAS
FROM EV_NOTAS
GROUP BY ID_OBRA
;
--------------------------------------------------------
--  DDL for View VW_OBRAS_AVALIADAS_PARE
--------------------------------------------------------

  CREATE OR REPLACE FORCE VIEW "A72201223"."VW_OBRAS_AVALIADAS_PARE" ("ID_OBRA", "ID_PARECERISTA", "DATA_RETORNO", "TEXTO_AVALIACAO", "JUSTIFICATIVA") AS 
  SELECT ID_OBRA, ID_PARECERISTA, DATA_RETORNO, TEXTO_AVALIACAO, JUSTIFICATIVA
FROM EV_AVALIACAO
;
--------------------------------------------------------
--  DDL for View VW_OBRAS_REVISADAS
--------------------------------------------------------

  CREATE OR REPLACE FORCE VIEW "A72201223"."VW_OBRAS_REVISADAS" ("ID_OBRA", "TITULO", "DIAGRAMACAO", "QNT_PALAVRAS", "RESUMO", "TIPO", "CPF_REVISOR", "RG_REVISOR", "NOME_REVISOR") AS 
  SELECT ID_OBRA, TITULO, DIAGRAMACAO, QNT_PALAVRAS, RESUMO, TIPO, CPF_REVISOR, RG_REVISOR, NOME_REVISOR
FROM EV_OBRA
WHERE CPF_REVISOR IS NOT NULL
;
